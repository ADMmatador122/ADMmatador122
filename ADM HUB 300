local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "adm hub " .. Fluent.Version,
    TabWidth = 160, 
    Size = UDim2.fromOffset(580, 460), 
    Theme = "Dark"
})

local Tabs = {
    Main = Window:AddTab({ Title = "Script" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

-- Adicionando parágrafo
Tabs.Main:AddParagraph({
    Title = "davi", 
    Content = "e bem aqui"
})

-- Botões
Tabs.Main:AddButton({
    Title = "Fly Script", 
    Callback = function()
        -- Fly Script
        loadstring(game:HttpGet('https://pastebin.com/raw/YSL3xKYU'))()
    end
})

Tabs.Main:AddButton({
    Title = "Inf Jump", 
    Callback = function()
        -- Inf Jump Script
        loadstring(game:HttpGet("https://raw.githubusercontent.com/HeyGyt/infjump/main/main"))()
    end
})

Tabs.Main:AddButton({
    Title = "Speed 200", 
    Callback = function()
        -- Speed 200 Script
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        -- Aumentando a velocidade para 200
        humanoid.WalkSpeed = 200
    end
})

Tabs.Main:AddButton({
    Title = "Speed 16", 
    Callback = function()
        -- Define a velocidade para 16
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        -- Definindo a velocidade para 16
        humanoid.WalkSpeed = 16
    end
})

Tabs.Main:AddButton({
    Title = "Infinity Yield", 
    Callback = function()
        -- Infinity Yield Script
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end
})

Tabs.Main:AddButton({
    Title = "Fling", 
    Callback = function()
        -- Fling Script: Fling outros jogadores longe, mas não o jogador atual
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        -- Criar uma função para fling os outros jogadores
        for _, target in ipairs(game.Players:GetPlayers()) do
            if target ~= player then
                local targetCharacter = target.Character
                if targetCharacter and targetCharacter:FindFirstChild("HumanoidRootPart") then
                    -- Fling: aplicando uma força para lançar o outro jogador
                    local rootPart = targetCharacter:FindFirstChild("HumanoidRootPart")
                    if rootPart then
                        local flingForce = Instance.new("BodyVelocity")
                        flingForce.MaxForce = Vector3.new(100000, 100000, 100000)
                        flingForce.Velocity = (rootPart.Position - character.HumanoidRootPart.Position).unit * 100
                        flingForce.Parent = rootPart
                        game.Debris:AddItem(flingForce, 0.5)  -- Remove a força após 0.5 segundos
                    end
                end
            end
        end
    end
})

-- Botão colorido com imagem que movimenta o personagem
Tabs.Main:AddButton({
    Title = "Mover Personagem",
    Callback = function()
        -- Criar um botão colorido pequeno com imagem na tela
        local screenGui = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
        screenGui.Name = "MoverButtonGui"
        
        local moveButton = Instance.new("TextButton")
        moveButton.Size = UDim2.new(0, 50, 0, 50)
        moveButton.Position = UDim2.new(0.5, -25, 0.5, -25)  -- Colocando o botão no centro da tela
        moveButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)  -- Cor do botão
        moveButton.Text = ""
        
        -- Defina a imagem do botão (adicione uma imagem URL válida)
        local buttonImage = Instance.new("ImageLabel")
        buttonImage.Size = UDim2.new(1, 0, 1, 0)
        buttonImage.Position = UDim2.new(0, 0, 0, 0)
        buttonImage.Image = "rbxassetid://YOUR_IMAGE_ID"  -- Troque "YOUR_IMAGE_ID" pelo ID da imagem
        buttonImage.Parent = moveButton
        
        moveButton.Parent = screenGui
        
        -- Movimentar o personagem de acordo com a posição do botão
        moveButton.MouseMoved:Connect(function(x, y)
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            
            -- Checando a posição do botão na tela
            local buttonPosition = moveButton.Position
            local screenWidth = game:GetService("Workspace").CurrentCamera.ViewportSize.X
            local screenHeight = game:GetService("Workspace").CurrentCamera.ViewportSize.Y

            -- Movimentar personagem para os lados quando o botão se move
            if buttonPosition.X.Scale > 0.5 then
                humanoid:MoveTo(character.HumanoidRootPart.Position + Vector3.new(1, 0, 0))  -- Move para direita
            elseif buttonPosition.X.Scale < 0.5 then
                humanoid:MoveTo(character.HumanoidRootPart.Position + Vector3.new(-1, 0, 0))  -- Move para esquerda
            end
        end)
        
        -- Fazer o personagem parar quando o botão é movido para o centro
        moveButton.MouseButton1Down:Connect(function()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            
            humanoid:MoveTo(character.HumanoidRootPart.Position)  -- Parar movimento
        end)
    end
})

-- Adicionando o botão Aimbot
